if [ "x$HAVE_COREAUDIO" != "xyes" ]; then
{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: Please take note that the building of the audio module for Mac OS X has been disabled. The Yate client will not have audio support. If you need audio, you need at least Mac OS X SDK 10.5 or something newer." >&5
$as_echo "$as_me: WARNING: Please take note that the building of the audio module for Mac OS X has been disabled. The Yate client will not have audio support. If you need audio, you need at least Mac OS X SDK 10.5 or something newer." >&2;}
fi
fi
